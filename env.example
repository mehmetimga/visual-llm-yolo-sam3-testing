# Ollama Configuration (Local Visual LLM)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_VISION_MODEL=minicpm-v:latest
OLLAMA_VISION_MODEL_FALLBACK=qwen2-vl:latest
OLLAMA_MAX_RETRIES=2
OLLAMA_TIMEOUT_MS=30000

# Qdrant Configuration (Vector DB for Visual Memory)
QDRANT_URL=http://localhost:6333
QDRANT_COLLECTION=ui_runs

# Feature Flags
VMS_ENABLED=true
SAM3_ENABLED=true
VGS_ENABLED=true

# Detector Service
DETECTOR_URL=http://localhost:8001

# DINOv3 Service
DINOV3_URL=http://localhost:8002

# SAM-3 Service
SAM3_URL=http://localhost:8003

# Web Runner
WEB_BASE_URL=http://localhost:3000
WEB_HEADLESS=true

# Mobile Runner (Appium)
APPIUM_URL=http://localhost:4723
FLUTTER_APP_PATH=./apps/demo-flutter/build/app/outputs/flutter-apk/app-debug.apk

# Output
OUT_DIR=./out

